<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/framework_fragment_background_layout"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:ignore="MissingDefaultResource">

    <FrameLayout
        android:id="@+id/framework_fragment_content_background"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@drawable/skin_main_bg"
        app:skin_enable="true" />

    <FrameLayout
        android:id="@+id/framework_fragment_title_bar_background"
        android:layout_width="match_parent"
        android:layout_height="@dimen/common_title_bar_height"
        android:background="@drawable/skin_title"
        app:skin_enable="true" />
</FrameLayout>