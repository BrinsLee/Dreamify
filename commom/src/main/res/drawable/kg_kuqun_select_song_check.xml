<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">

    <item android:state_checked="true" android:state_window_focused="false"
          android:state_enabled="true"
          android:drawable="@drawable/kg_kuqun_select_song_checked" />
    <item android:state_checked="false" android:state_window_focused="false"
          android:state_enabled="true"
          android:drawable="@drawable/kg_kuqun_select_song_uncheck" />

    <item android:state_checked="true" android:state_pressed="true"
          android:state_enabled="true"
          android:drawable="@drawable/kg_kuqun_select_song_checked" />
    <item android:state_checked="false" android:state_pressed="true"
          android:state_enabled="true"
          android:drawable="@drawable/kg_kuqun_select_song_uncheck" />

    <item android:state_checked="true" android:state_focused="true"
          android:state_enabled="true"
          android:drawable="@drawable/kg_kuqun_select_song_checked" />
    <item android:state_checked="false" android:state_focused="true"
          android:state_enabled="true"
          android:drawable="@drawable/kg_kuqun_select_song_uncheck" />

    <item android:state_checked="false"
          android:state_enabled="true"
          android:drawable="@drawable/kg_kuqun_select_song_uncheck" />
    <item android:state_checked="true"
          android:state_enabled="true"
          android:drawable="@drawable/kg_kuqun_select_song_checked" />

    <!-- Disabled states -->

    <item android:state_checked="true" android:state_window_focused="false"
          android:drawable="@drawable/kg_kuqun_select_song_checked" />
    <item android:state_checked="false" android:state_window_focused="false"
          android:drawable="@drawable/kg_kuqun_select_song_uncheck" />

    <item android:state_checked="true" android:state_focused="true"
          android:drawable="@drawable/kg_kuqun_select_song_checked" />
    <item android:state_checked="false" android:state_focused="true"
          android:drawable="@drawable/kg_kuqun_select_song_uncheck" />

    <item android:state_checked="false" android:drawable="@drawable/kg_kuqun_select_song_uncheck" />
    <item android:state_checked="true" android:drawable="@drawable/kg_kuqun_select_song_uncheck" />

</selector>